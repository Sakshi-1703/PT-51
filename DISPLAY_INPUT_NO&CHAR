ORG 00H
MOV P0,#0FFH
MOV A,P0
MOV R1,A
MOV 70H,R1
ANL A,#0FH
MOV 71H,A
SUBB A,#10
JNC DOWN
ACALL NUMBER_1
DOWN: ACALL CHARACTER_1

NUMBER_1:
MOV A,71H
ADD A,#48
MOV P1,A

CHARACTER_1:
MOV A,71H
ADD A,#55
MOV P1,A

MOV A,70H
ANL A,#0F0H
SWAP A
MOV 72H,A
SUBB A,#10
JNC DOWN1
ACALL NUMBER_2
DOWN1: ACALL CHARACTER_2

NUMBER_2:
MOV A,72H
ADD A,#48
MOV P2,A

CHARACTER_2:
MOV A,72H
ADD A,#55
MOV P2,A
END
